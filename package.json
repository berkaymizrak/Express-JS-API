    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.5.2",
    "morgan": "~1.9.1",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/node": "^7.18.10",
    "eslint": "^8.22.0",
    "prettier": "^2.7.1"
  }
}
