{
  "name": "express-js-api",
  "version": "1.0.0",
  "description": "Express JS API",
  "main": "src/app.js",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "yarn start:prod",
    "start:dev": "nodemon",
