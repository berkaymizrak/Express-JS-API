{
  "version": 2,
  "builds": [
    {
      "src": "./src/index-compiled.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "/.adminjs/**",
          "/src/locales/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/index-compiled.js"
    }
  ]
}